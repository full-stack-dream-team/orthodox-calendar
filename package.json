{
  "name": "orthodox-calendar",
  "version": "1.0.0",
  "main": "start.js",
  "description": "Simple node react boilerplate",
  "prettier": {},
  "scripts": {
    "server": "nodemon start.js",
    "client": "yarn --cwd client start",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "start": "node start.js",
    "heroku-postbuild": "YARN_PRODUCTION=false yarn --cwd client && yarn --cwd client build"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.0.0-rc.3",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.7",
    "nodemailer": "^6.4.6",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "puppeteer": "^3.0.4",
    "puppeteer-cluster": "^0.20.0",
    "validator": "^13.0.0"
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "nodemon": "^2.0.2"
  }
}
